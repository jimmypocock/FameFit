# FameFit Push Service Environment Variables
# Copy this file to .env and fill in your values

# CloudKit Configuration
CLOUDKIT_CONTAINER_ID=iCloud.com.jimmypocock.FameFit
CLOUDKIT_API_TOKEN=your_cloudkit_api_token_here
CLOUDKIT_ENVIRONMENT=development

# APNS Configuration
APNS_KEY_ID=your_apns_key_id
APNS_TEAM_ID=your_apple_team_id
APNS_BUNDLE_ID=com.jimmypocock.FameFit
APNS_KEY_PATH=./certs/AuthKey.p8
APNS_PRODUCTION=false

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password
REDIS_DB=0

# Service Configuration
NODE_ENV=development
LOG_LEVEL=debug
PORT=3000

# Monitoring (Optional)
GRAFANA_PASSWORD=admin
METRICS_ENABLED=true

# Rate Limiting
USER_RATE_LIMIT_HOUR=50
USER_RATE_LIMIT_DAY=200

# Notification Settings
BATCH_SIZE=100
BATCH_INTERVAL=5000
MAX_RETRIES=3
RETRY_DELAY=2000